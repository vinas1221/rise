{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "lib",
    "listEmittedFiles": true,
    "declaration": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "dom"
    ]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/**/*"]
}
